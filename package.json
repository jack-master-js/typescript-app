{
    "name": "express-typescript-starter",
    "version": "0.1.0",
    "description": "A starting point for Node.js express apps with TypeScript",
    "scripts": {
        "build": "tsc",
        "serve": "node dist/server.js",
        "start": "npm run serve",
        "watch-node": "nodemon dist/server.js",
        "watch-ts": "tsc -w",
        "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "apidoc": "apidoc -c apidoc.json -i src/ -o public/apidoc/"
    },
    "dependencies": {
        "body-parser": "1.19.0",
        "compression": "^1.7.4",
        "connect-mongo": "^4.6.0",
        "connect-timeout": "^1.9.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "8.2.0",
        "express": "4.17.1",
        "express-session": "^1.17.2",
        "ioredis": "^4.28.0",
        "jsonwebtoken": "^8.5.1",
        "log4js": "^6.3.0",
        "md5": "^2.3.0",
        "mongodb": "^4.1.4",
        "multer": "^1.4.3",
        "node-fetch": "^3.0.0",
        "node-xlsx": "^0.17.2",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "response-time": "^2.3.2",
        "typeorm": "^0.2.38",
        "ws": "^8.2.3"
    },
    "devDependencies": {
        "@types/body-parser": "1.19.0",
        "@types/compression": "^1.7.2",
        "@types/concurrently": "5.2.1",
        "@types/connect-timeout": "^0.0.35",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "4.17.11",
        "@types/express-session": "^1.17.4",
        "@types/ioredis": "^4.28.1",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/md5": "^2.3.1",
        "@types/multer": "^1.4.7",
        "@types/node": "14.14.25",
        "@types/node-xlsx": "^0.15.2",
        "@types/request": "2.48.5",
        "@types/request-promise": "4.1.47",
        "@types/response-time": "^2.3.5",
        "@types/ws": "^8.2.0",
        "concurrently": "6.0.2",
        "nodemon": "^2.0.7",
        "ts-node": "9.1.1",
        "typescript": "4.1.3"
    }
}
